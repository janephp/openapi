{
    "swagger": "2.0",
    "info": {
        "title": "Docker Remote API",
        "description": "The API for each docker installation.",
        "termsOfService": "http://example.com/tos/",
        "version": "v1.19"
    },
    "host": "unix:///var/run/docker.sock",
    "basePath": "/",
    "schemes": [
        "unix",
        "http",
        "https"
    ],
    "produces": [
        "application/json"
    ],
    "consumes": [
        "application/json"
    ],
