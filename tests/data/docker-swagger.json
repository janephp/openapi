                    }
                },
                "Entrypoint": {
                    "type": ["array", "string"],
                    "items": {
                        "type": "string"
                    }
                },
                "Mounts": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/mount"
                    }
                },
                "WorkingDir": {
                    "type": "string"
                },
                "NetworkDisabled": {
                    "type": "boolean"
                },
                "MacAddress": {
                    "type": "string"
                },
                "ExposedPorts": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "object",
                        "enum": [
                            {}
                        ],
                        "default": {}
                    }
                },
                "HostConfig": {
                    "$ref": "#/definitions/HostConfig"
                }
            }
        },
        "NetworkConfig": {
            "type": "object",
            "properties": {
                "Bridge": {
